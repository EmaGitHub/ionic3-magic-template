{
  "name": "ionic3template",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic clean",
    "build": "ionic build",
    "lint": "eslint --fix ./src --ext .ts",
    "serve:mocks": "ENV=mocks ionic serve -p 8080",
    "serve:dev": "ENV=dev ionic serve -p 8080",
    "serve:prod": "ENV=prod ionic serve -p 8080",
    "build:android:dev": "ENV=dev ionic cordova build android --prod --no-interactive --release",
    "build:android:prod": "ENV=prod ionic cordova build android --prod --no-interactive --release",
    "build:ios:dev": "ENV=dev ionic cordova build ios --prod --no-interactive",
    "build:ios:prod": "ENV=prod ionic cordova build ios --prod --no-interactive --release",
    "run:ios:dev": "ENV=dev ionic cordova run ios --no-interactive",
    "run:android:dev": "ENV=dev ionic cordova run android --no-interactive",
    "start-mocks": "./node_modules/.bin/distribute serve ./mocks",
    "ionic:watch:before": "node ./hooks/environment-selector.js",
    "ionic:build:before": "node ./hooks/environment-selector.js"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "10.2.5",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic-native/app-version": "4.12.2",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/device": "4.5.3",
    "@ionic-native/dialogs": "4.12.2",
    "@ionic-native/firebase": "4.7.0",
    "@ionic-native/geolocation": "4.7.0",
    "@ionic-native/globalization": "4.5.3",
    "@ionic-native/in-app-browser": "4.7.0",
    "@ionic-native/keyboard": "4.5.3",
    "@ionic-native/launch-navigator": "4.7.0",
    "@ionic-native/network": "4.5.3",
    "@ionic-native/screen-orientation": "4.7.0",
    "@ionic-native/spinner-dialog": "4.5.3",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "@ngx-translate/core": "9.1.1",
    "@types/crypto-js": "3.1.40",
    "@types/semver": "5.5.0",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "cordova-plugin-app-version": "0.1.9",
    "cordova-plugin-device": "2.0.1",
    "cordova-plugin-dialogs": "2.0.1",
    "cordova-plugin-ionic-keyboard": "2.0.5",
    "cordova-plugin-native-spinner": "1.1.3",
    "cordova-plugin-network-information": "2.0.1",
    "cordova-plugin-splashscreen": "5.0.2",
    "crypto-js": "3.1.9-1",
    "ionic-angular": "3.9.2",
    "ionic-plugin-keyboard": "2.2.1",
    "ionicons": "3.0.0",
    "loki-cordova-fs-adapter": "1.0.2",
    "lokijs": "1.5.3",
    "mobile-app-distribution": "0.5.4",
    "rxjs": "5.5.2",
    "semver": "5.5.1",
    "sw-toolbox": "3.6.0",
    "webpack-merge": "4.1.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.8",
    "@types/lokijs": "1.5.1",
    "eslint": "5.3.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-prettier": "3.0.1",
    "prettier": "1.17.0",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.4.0",
    "typedoc": "0.11.1",
    "typescript": "2.7.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-device": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-splashscreen": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-globalization": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-geolocation": {
        "GEOLOCATION_USAGE_DESCRIPTION": "To locate you"
      },
      "cordova-plugin-screen-orientation": {},
      "cordova-plugin-ios-disableshaketoedit": {},
      "cordova-plugin-compat": {},
      "cordova-plugin-ionic-keyboard": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-native-spinner": {},
      "cordova-plugin-app-version": {},
      "cordova-plugin-dialogs": {}
    }
  },
  "config": {
    "ionic_source_map_type": "source-map",
    "ionic_webpack": "./webpack.config.js"
  }
}
